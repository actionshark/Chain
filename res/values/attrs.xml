<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="chain">
		<attr name="direct">
			<enum
				name="left"
				value="0" />
			<enum
				name="right"
				value="1" />
			<enum
				name="top"
				value="2" />
			<enum
				name="bottom"
				value="3" />
		</attr>
		<attr
			name="start_color"
			format="color" />
		<attr
			name="end_color"
			format="color" />
	</declare-styleable>
</resources>
